{
  "name": "supplylink",
  "version": "1.0.0",
  "description": "SupplyLink - Supply Chain Management Platform",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd client && npm run preview",
    "install:client": "cd client && npm install --legacy-peer-deps",
    "build:client": "cd client && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 